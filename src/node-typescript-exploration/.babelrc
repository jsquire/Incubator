{
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "node": "current",
          "browsers": "last 3 versions"
        }
      }],
      "@babel/stage-3",
      "@babel/typescript"
    ],
    "plugins": [
        ["module-resolver", {
            "extensions": [".js", ".ts"],
            "root": ["./src"]
        }]
    ]
  }